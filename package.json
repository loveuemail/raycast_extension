{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "e-mail.love",
    "title": "e-mail.love",
    "description": "Generate and manage your e-mail.love aliases from your keyboard.",
    "icon": "command-icon.png",
    "author": "http.james",
    "contributors": [
      "pernielsentikaer"
    ],
    "categories": [
      "Productivity"
    ],
    "license": "MIT",
    "commands": [
      {
        "name": "generate-alias",
        "title": "Generate Alias",
        "subtitle": "e-mail.love",
        "description": "Generate a random e-mail.love alias",
        "mode": "no-view"
      },
      {
        "name": "list-aliases",
        "title": "List Aliases",
        "subtitle": "e-mail.love",
        "description": "List and search e-mail.love aliases",
        "mode": "view"
      },
      {
        "name": "edit-alias",
        "title": "Edit Alias",
        "subtitle": "e-mail.love",
        "description": "Edit an e-mail.love alias",
        "mode": "view"
      }
    ],
    "dependencies": {
      "@raycast/api": "^1.39.3",
      "node-fetch": "^3.2.10"
    },
    "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "eslint": "^7.32.0",
      "eslint-config-prettier": "^8.3.0",
      "prettier": "^2.5.1",
      "typescript": "^4.4.3"
    },
    "scripts": {
      "build": "ray build -e dist",
      "dev": "ray develop",
      "fix-lint": "ray lint --fix",
      "lint": "ray lint",
      "publish": "ray publish"
    },
    "preferences": [
      {
        "name": "apiKey",
        "description": "The API key used to interact with e-mail.love's API.",
        "type": "password",
        "required": true,
        "title": "e-mail.love API Key"
      }
    ]
  }
  
